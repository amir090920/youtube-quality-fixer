{
  "manifest_version": 3,
  "name": "مثبت جودة يوتيوب",
  "version": "2.0.0",
  "description": "إضافة متصفح تقوم بتثبيت جودة يوتيوب",
  
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://youtube.com/*"
  ],
  
  "background": {
    "service_worker": "background.js"
  },
  
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["injector.js"],
      "run_at": "document_start"
    }
  ],
  
  "web_accessible_resources": [
    {
      "resources": ["youtube-controller.js"],
      "matches": ["*://*.youtube.com/*"]
    }
  ],
  
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "data:image/png;base64,"
    }
  },
  
  "icons": {
    "128": "data:image/png;base64,"
  }
}
